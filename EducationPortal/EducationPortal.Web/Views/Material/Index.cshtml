@model IEnumerable<EducationPortal.BLL.DTO.MaterialDTO>
@{
    ViewData["Title"] = "Materials";
}
<h2 class="text-center">All Materials</h2>
<div class="text-center">
    <p><a asp-action="Create">Add New Material</a></p>
    @if (Model.Count() == 0)
    {
        <h2>No Materials</h2>
    }
    else
    {
        @await Html.PartialAsync("_ShowMaterialPartial", Model)
    }
</div>