@model IEnumerable<EducationPortal.BLL.DTO.SkillDTO>

    @{
    ViewData["Title"] = "Skills";
}
<h2 class="text-center">All Skills</h2>

<div class="text-center">
    <p><a asp-action="Create">Add New Skill</a></p>
    @if (Model.Count() == 0)
    {
        <h2>No Materials</h2>
    }
    else
    {
        @await Html.PartialAsync("_SkillsPartial", Model)
    }
</div>